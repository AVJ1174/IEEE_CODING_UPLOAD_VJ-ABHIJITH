Q E1 ) 
Answer) 

import java.util.* ;
class Solution {
    public static int climbStairs(int n) {
        if (n==1) {
            return 1;
        }
        if (n==2) {
            return 2 ;
        }
        return climbStairs(n-1) + climbStairs(n-2) ;
    }
    public static void main(String s [] ) {
        Scanner input = new Scanner(System.in) ;
        System.out.println("Enter Number of Stairs to Climb") ;
        int n=input.nextInt() ;
        int ways=climbStairs(n);
        System.out.println("The number of ways in which we can climb "+n+" stairs is: "+ways) ;
    }
}

Q E2 )
Answer ) 

import java.util.* ;
class SolutionE2 {
    public static int[] smallerNumbersThanCurrent( int [] nums) {
        int count = 0;
        int[] counter = new int[nums.length] ;
        for (int i = 0; i < nums.length; i++) {
            for (int j = 0; j < nums.length; j++) {
                if (nums[j] != nums[i] && nums[j] < nums[i])
                    count++;
            }
            counter[i] = count;
            count=0;
        }
        return counter;
    }

    public static void main (String S[] ) {
        System.out.println("Enter Number of elements in Array :");
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        System.out.println("Enter "+ n +" Elements : ");
        int [] arr = new int[n] ;
        for (int i = 0; i < n; i++) {
            arr[i] = input.nextInt();
        }
        System.out.println("Array Has been Created");
        int[] answer = smallerNumbersThanCurrent(arr);
        System.out.println("The Array of Number of smaller numbers of each element in entered Array is : " + Arrays.toString(answer));

    }
}

Q E-3)
Answer ) 

import java.util.* ;
public class E3{
    public static int[] twoSum(int [] nums , int target){
        int x = nums.length;
        int [] result = new int[2];
        for(int i = 0 ; i<x;i++){
            for(int j=i+1;j<x;j++){
                if(nums[i]+nums[j]==target){
                    result[0]=i ;
                    result[1]=j ;
                }
            }
        }
        return result ;
    }
    public static void main(String s[]){
        Scanner input = new Scanner(System.in);
        System.out.println("Enter number of elements: ");
        int n = input.nextInt();
        System.out.println("Enter "+n+" Elements:");
        int [] nums = new int[n];
        for(int i=0;i<n;i++){
            nums[i]= input.nextInt();
        }
        System.out.println("Enter Target : ") ;
        int target= input.nextInt();
        int [] result = twoSum(nums,target) ;
        if(result!=null){
            System.out.println("The position of the 2 elements is : "+ Arrays.toString(result)) ;
        }
        else {
            System.out.println("No 2 elements add up to the target");
        }
    }
}



